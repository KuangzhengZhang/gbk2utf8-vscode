{
  "name": "GBK2UTF8",
  "displayName": "GBK to UTF8 for vscode",
  "description": "Convert file encode from gbk to utf8 for vscode",
  "publisher": "buuug7",
  "icon": "icon.png",
  "author": "buuug7",
  "license": "MIT",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "gbk",
    "utf8",
    "gbk utf8"
  ],
  "homepage": "https://github.com/buuug7/gbk2utf8-vscode/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/buuug7/gbk2utf8-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/buuug7/gbk2utf8-vscode/issues"
  },
  "activationEvents": [
    "onCommand:GBK_TO_UTF8.generate"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "GBK_TO_UTF8.generate",
        "title": "Generate encode to utf8"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "iconv-lite": "^0.6.2",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "ts-loader": "^8.0.14",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  }
}
